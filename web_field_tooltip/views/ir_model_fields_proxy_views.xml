<odoo>

    <record model="ir.ui.view" id="ir_model_fields_proxy_form_view">
        <field name="model">ir.model.fields.proxy</field>
        <field name="arch" type="xml">
             <form duplicate="0" create="0" edit="0">
                 <group name="main">
                     <group name="group_main_left">
                         <field name="name" />
                         <field name="description" />
                         <field name="relation" />
                     </group>
                     <group name="group_main_right">
                         <field name="ttype" />
                         <field name="index" />
                         <field name="store" />
                         <field name="readonly" />
                     </group>
                 </group>
             </form>
        </field>
    </record>
    <record model="ir.ui.view" id="ir_model_fields_proxy_tree_view">
        <field name="model">ir.model.fields.proxy</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="description" />
                <field name="ttype" />
                <field name="index" />
                <field name="store" />
                <field name="readonly" />
                <field name="relation" />
            </tree>
        </field>
    </record>

</odoo>
