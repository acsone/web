<odoo>

    <record model="ir.ui.view" id="ir_model_proxy_form_view">
        <field name="model">ir.model.proxy</field>
        <field name="arch" type="xml">
             <form duplicate="0" create="0" edit="0">
                 <group name="main">
                     <group name="group_main_left">
                         <field name="description" />
                         <field name="name" />
                     </group>
                     <group name="group_main_right">
                         <field name="state" />
                         <field name="transient" />
                     </group>
                 </group>
             </form>
        </field>
    </record>
    <record model="ir.ui.view" id="ir_model_proxy_tree_view">
        <field name="model">ir.model.proxy</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="description" />
                <field name="state" />
                <field name="transient" />
            </tree>
        </field>
    </record>

</odoo>
